|| <%= link_to "Welcome Page", root_path %> ||
<%= render partial: '/layouts/errors', locals: {object: @user} %>

<h1> Sign up </h1>
<%= form_for @user do |f| %>
    <fieldset><br>
        <%= f.label :name %><br>
        <%= f.text_field :name %><br><br>

        <%= f.label :email %><br>
        <%= f.text_field :email %><br><br>

        <%= f.label :password %><br>
        <%= f.password_field :password %><br><br>
    <%= f.submit "Sign Up" %> 
    </fieldset><br>
<% end %>

OR  
<br><br><%= button_to "Sign Up With Google", '/auth/google_oauth2' %>