<!DOCTYPE html>
<html>
  <head>
    <title>ProjectFramework</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <div>
    <%= button_to "Log In With Google", '/auth/google_oauth2' %>
    <% if current_user %>
      <%= link_to "Log Out", logout_path, method: :delete%><br>
      <%= link_to "Your Projects", projects_path %><br>
      <%= link_to "Add A New Project", new_project_path %><br><br>
    <% end %>
  </div>

    <%= yield %>
    
  </body>
</html>
